<h1>Users </h1>
<%= link_to "Create New User", new_user_path %>
<% @users.each do |user| %>

<p>Name: <%= user.name %> 
<%= link_to "Show", user_path(user)  %> 
<%= link_to "Edit", edit_user_path(user) %>
<%= link_to "delete", user_path(user) , data: {turbo_method: :delete, turbo_confirm: "Are you sure want to delete ?"} %>
<% end %>


